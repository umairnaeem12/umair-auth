datasource db {
  provider = "postgresql"
  url      = env("MAIN_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id        String   @id @default(cuid())
  dbUrl     String
  jwtSecret String
  createdAt DateTime @default(now())
  
  @@map("Project")  // Explicit table name mapping
}
