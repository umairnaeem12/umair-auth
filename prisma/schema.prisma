generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["clientExtensions"]
}

datasource db {
  provider = "postgresql"
  url      = env("MAIN_DATABASE_URL")
}

model Project {
  id        String   @id @default(cuid())
  dbUrl     String
  jwtSecret String
  createdAt DateTime @default(now())

  @@map("Project")
}
